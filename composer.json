{
    "name": "stechstudio/laravel-route-context",
    "description": "Provide additional route context that gets treated as normal route parameters",
    "keywords": [
        "stechstudio",
        "laravel",
        "laravel-route-context"
    ],
    "homepage": "https://github.com/stechstudio/laravel-route-context",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Szobody",
            "email": "joseph@stechstudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "STS\\RouteContext\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "STS\\RouteContext\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "STS\\RouteContext\\RouteContextServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}